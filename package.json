{
  "name": "ballup",
  "version": "1.0.0",
  "description": "Create teams for pickup basketball games",
  "main": "index.js",
"scripts": {
  "start": "node app.js",
  "server": "nodemon app.js --ignore './frontend/'",
  "server:debug": "nodemon --inspect app.js",
  "frontend-install": "npm install --prefix frontend",
  "frontend": "npm start --prefix frontend",
  "dev": "concurrently \"npm run server\" \"npm run frontend\"",
  "dev-debugger": "concurrently \"npm run server:debug\" \"npm run frontend\""
},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkirkpatrick99/BallUP.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dkirkpatrick99/BallUP/issues"
  },
  "homepage": "https://github.com/dkirkpatrick99/BallUP#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.9.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-redux": "^7.2.0",
    "redux-logger": "^3.0.6",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
